{
  "name": "goodbuy-monorepo",
  "private": true,
  "workspaces": [
    "backend/src",
    "mobile-app/src"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:mobile\"",
    "dev:backend": "npm --prefix backend/src run start",
    "dev:mobile": "npm --prefix mobile-app/src run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
